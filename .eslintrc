{
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": ["eslint:recommended", "eslint-config-prettier", "plugin:react/recommended", "plugin:react/jsx-runtime"],
    "rules": {
      "no-unused-vars": ["error", {}]
    },
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true
    }
  }